		<QuestsSorted NameClass="" Action="EndIf" />
		
		<QuestsSorted NameClass="ObjectManager.Me.Level &gt;= 7 &amp;&amp; ObjectManager.Me.Level &lt;= 11" Action="If" />
			<QuestsSorted NameClass="AllianceQuestsDroidz\(07-10)ElwynnForest.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
		
		<QuestsSorted NameClass="ObjectManager.Me.Level &gt;= 10 &amp;&amp; ObjectManager.Me.Level &lt;= 15" Action="If" />
			<QuestsSorted NameClass="AllianceQuestsDroidz\(10-15)Westfall.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
		
		<QuestsSorted NameClass="ObjectManager.Me.Level &gt;= 15" Action="If" />
			<QuestsSorted NameClass="AllianceQuestsDroidz\(15-16)RedridgeMountains.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
	
	<QuestsSorted NameClass="" Action="EndIf" />
	
	<QuestsSorted NameClass="ObjectManager.Me.PlayerRace == PlayerFactions.NightElf" Action="If" />
	
		<QuestsSorted NameClass="ObjectManager.Me.Level &lt;= 5" Action="If" />
			<QuestsSorted NameClass="AllianceQuestsDroidz\(01-05)NightElf.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
		
		<QuestsSorted NameClass="ObjectManager.Me.Level &gt;= 5 &amp;&amp; ObjectManager.Me.Level &lt;= 12" Action="If" />
			<QuestsSorted NameClass="AllianceQuestsDroidz\(05-11)Teldrassil.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
		
		<QuestsSorted NameClass="ObjectManager.Me.Level &gt;= 11" Action="If" />
			<QuestsSorted NameClass="AllianceQuestsDroidz\(11-16)Darkshore.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
	
	<QuestsSorted NameClass="" Action="EndIf" />
	
