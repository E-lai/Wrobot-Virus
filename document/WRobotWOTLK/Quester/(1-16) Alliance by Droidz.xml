			<QuestsSorted NameClass="AllianceQuestsDroidz\(01-05)Human.xml" Action="LoadProfile" />
		<QuestsSorted NameClass="" Action="EndIf" />
		
		<QuestsSorted NameClass="ObjectManager.Me.Level &gt;= 5 &amp;&amp; ObjectManager.Me.Level &lt;= 7" Action="If" />
